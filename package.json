{
  "name": "cali-lew",
  "version": "0.0.1",
  "private": true,
  "homepage": "http://calilew.github.io",
  "devDependencies": {
    "react-scripts": "0.2.1"
  },
  "dependencies": {
    "basscss": "^8.0.3",
    "classnames": "2.2.5",
    "masonry": "0.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-imageloader": "2.1.0",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-swipeable": "3.6.0",
    "redux": "^3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "schematizr": "4.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy",
    "eject": "react-scripts eject",
    "move-css": "cp ./build/static/css/*.css ./dist/css/main.css && cp ./build/static/css/*.map ./dist/css/main.css.map",
    "move-js": "cp ./build/static/js/*.js ./dist/js/main.js && cp ./build/static/js/*.map ./dist/js/main.js.map",
    "copy-img": "cp -R ./img ./dist",
    "copy": "npm run move-css && npm run move-js"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
